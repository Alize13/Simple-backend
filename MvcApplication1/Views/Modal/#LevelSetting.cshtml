@using MvcApplication1.Models;
@model DB_MemberLevel


<div class="card">
    <h3></h3>
    @Html.HiddenFor(m => m.LevelID)
    <div>
        @Html.LabelFor(m => m.MemberLevel, new { @class = "col-lg-5" })
        @Html.TextBoxFor(m => m.MemberLevel, new { disabled = "disabled", @class = "col-lg-6" })
    </div>
    <div>
        @Html.LabelFor(m => m.LevelName, new { @class = "col-lg-5" })
        @Html.TextBoxFor(m => m.LevelName, new
        {
            name = "LevelName",
            id = "LevelName",
            @class = "col-lg-6",
            @onkeypress = "SpecialKeyPress()",
            @onchange = "ChangeSpecial($(this))",
            maxlength = 50
        })
    </div>
</div>



